{
    "name": "alphabetic-posts-directory-wp",
    "version": "1.0.0",
    "description": "Creates a directory-style layout and displays posts ordered alphabetically with an A-Z selector to filter posts",
    "main": "index.js",
    "scripts": {
        "lint:scss": "stylelint 'assets/styles/src/**/*.scss' --syntax scss",
        "lint:scss:fix": "stylelint 'assets/styles/src/**/*.scss' --syntax scss --fix",
        "lint:js": "eslint assets/scripts/src/**/*.{js,jsx}",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint": "npm run lint:js && npm run lint:scss",
        "build": "npm run lint && webpack --config ./webpack/production.js",
        "buildonly": "webpack --config ./webpack/production.js",
        "dev": "webpack --watch --config ./webpack/development.js",
        "wp-pot": "wp-pot --src 'lib/**/*.php' --dest-file 'languages/sqh.pot' --package 'sqh' --domain 'sqh' --last-translator 'Adrian Emil Tudorache <adrian.tudorache@cloudweb.ch>' --team 'Team <info@cloudweb.ch' --bug-report 'https://www.cloudweb.ch' --write-file"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/railmedia/alphabetic-posts-directory-wp"
    },
    "author": "Adrian Emil Tudorache (adrian@railmedia.es)",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/plugin-transform-runtime": "^7.23.4",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@wordpress/babel-preset-default": "^7.31.0",
        "@wordpress/browserslist-config": "^5.30.0",
        "@wordpress/prettier-config": "^3.4.0",
        "autoprefixer": "^10.4.16",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "clean-webpack-plugin": "^4.0.0",
        "compression-webpack-plugin": "^10.0.0",
        "css-loader": "^6.8.1",
        "css-mqpacker": "^7.0.0",
        "eslint": "^8.54.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-wordpress": "^2.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.3",
        "jquery": "^3.7.1",
        "lint-staged": "^15.1.0",
        "loadash": "^1.0.0",
        "mini-css-extract-plugin": "^2.7.6",
        "npm-run-all": "^4.1.5",
        "pixrem": "^5.0.0",
        "postcss-loader": "^7.3.3",
        "postcss-mixins": "^9.0.4",
        "postcss-nested": "^6.0.1",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.1.0",
        "prettier-eslint": "^16.1.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "stylelint": "^15.11.0",
        "stylelint-webpack-plugin": "^4.1.1",
        "svg-spritemap-webpack-plugin": "^4.5.0",
        "svgo": "^3.0.5",
        "terser-webpack-plugin": "^5.3.9",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "^5.10.0",
        "wp-pot": "^1.10.2",
        "wp-pot-cli": "^1.5.0"
    }
}
